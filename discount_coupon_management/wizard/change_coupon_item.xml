<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_change_coupon_bunch" model="ir.ui.view">
            <field name="name">change.coupon.bunch</field>
			<field name="model">change.coupon.detail</field>
			<field name="arch" type="xml">
                <form string="Coupon">
                    <h2>Are you sure want change number ofcoupon?</h2> 
                    <field name="bunch" required="1"></field>                   
                    <footer>
                        <span>
                            <button string="Yes" type="object"
                                    name="state_bunch" class="oe_highlight"/>
                            or
                            <button special="cancel" string="No" type="object"/>
                        </span>
                    </footer>
                </form>
			</field>
		</record>
		<act_window id="action_set_coupon_view" multi="False"
                    name="Change Coupon Bunch"  res_model="change.coupon.detail" src_model="make.discount.coupon"
					view_mode="form" target="new" view_type="form"/>
	</data>
</odoo>