<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_change_coupon_form" model="ir.ui.view">
            <field name="name">change.coupon.form</field>
			<field name="model">make.coupon.detail</field>
			<field name="arch" type="xml">
                <form string="Coupon">
                    <h2>Are you sure want change redeem coupon?</h2>                    
                    <footer>
                        <span>
                            <button string="Yes" type="object"
                                    name="state_redeem" class="oe_highlight"/>
                            or
                            <button special="cancel" string="No" type="object"/>
                        </span>
                    </footer>
                </form>
			</field>
		</record>
		<act_window id="action_set_coupon_view" multi="True"
                    name="Coupon"  res_model="make.coupon.detail" src_model="coupon.detail"
					view_mode="form" target="new" view_type="form"/>
	</data>
</odoo>